{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/v1/user/(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/api/v1/admin/(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/api/v1/course/(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}